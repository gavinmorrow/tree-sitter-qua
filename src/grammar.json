    },
    "bool": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "[0-9]+(\\.[0-9]+)?"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "identifier": {
      "type": "PATTERN",
      "value": "([a-z]|[A-Z]|_)([a-z]|[A-Z]|[0-9]|_)*"
    },
    "comment": {
      "type": "PATTERN",
      "value": "\\/\\/.*\\$"
    }
