        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "identifier"
              },
              {
                "type": "STRING",
                "value": ","
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "logic_or"
        },
        {
          "type": "SYMBOL",
          "name": "logic_and"
        },
        {
          "type": "SYMBOL",
          "name": "equality"
        },
        {
          "type": "SYMBOL",
          "name": "comparison"
        },
        {
          "type": "SYMBOL",
